{
  "expo": {
    "name": "Bemiter",
    "slug": "Bemiter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#369ae1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["android","ios"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bemiter.app"
    },
    "android": {
      "package": "com.bemiter.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "44.0.0"
  },
  "name": "bemiter"
}